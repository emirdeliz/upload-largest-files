{
	"name": "upload-largest-files",
	"version": "1.0.0",
	"description": "Upload largest files",
	"type": "commonjs",
	"moduleResolution": "node",
	"exports": "./dist/upload-largest-files.js",
	"repository": "https://github.com/emirdeliz/upload-largest-files",
	"author": "Emir Marques - <emirdeliz@gmail.com>",
	"license": "MIT",
	"scripts": {
		"build:types": "tsc ./src/upload-largest-files.ts --outDir dist --declaration --emitDeclarationOnly",
		"build:dist": "rm -rf dist && swc src --out-dir dist && yarn build:types",
		"test": "jest --clearCache && jest",
		"test:watch": "jest --clearCache && jest --watchAll --runInBand",
		"dev": "rm -rf dist && swc src --watch --out-dir dist",
		"lint:check": "eslint . --ext .ts --fix",
		"clear:dependencies": "rm -rf node_modules && rm -rf yarn.lock",
		"install:dev": "yarn clear:dependencies && yarn install",
		"start:example": "yarn --cwd example start"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.3.14",
		"@swc/helpers": "^0.4.12",
		"@swc/jest": "^0.2.24",
		"@types/jest": "^29.2.2",
		"@types/node": "^18.11.9",
		"@typescript-eslint/eslint-plugin": "^5.31.0",
		"@typescript-eslint/parser": "^5.31.0",
		"chokidar": "^3.5.3",
		"eslint": "^8.20.0",
		"jest": "^29.3.1",
		"jest-environment-jsdom": "^29.3.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.7.4"
	}
}
